# Environment variables for local development
# Copy this file to .env.local and fill in the values

# Firebase Admin SDK service account JSON (paste as single line)
FIREBASE_SERVICE_ACCOUNT=

# Admin API key for protected endpoints
ADMIN_API_KEY=your-secret-admin-key-here

# Firebase Storage Bucket (format: project-id.firebasestorage.app or project-id.appspot.com)
FIREBASE_STORAGE_BUCKET=ddupvc.firebasestorage.app
